{
    "name": "ekstremedia/laravel-netatmo-weather",
    "license": "MIT",
    "description": "A Laravel package for integrating Netatmo Weather Station API with your Laravel application",
    "keywords": [
        "laravel",
        "netatmo",
        "weather",
        "api",
        "weather-station"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Terje Nesthus",
            "email": "terje@nesthus.no"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ekstremedia\\NetatmoWeather\\": "src/"
        },
        "classmap": [
            "database/factories",
            "database/seeders"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ekstremedia\\NetatmoWeather\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ekstremedia\\NetatmoWeather\\NetatmoWeatherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
